    "prop-types": "^15.5.6",
    "react": "^15.0.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^15.0.0",
    "react-quill": "^1.0.0-rc-1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redbox-react": "^1.2.10",
