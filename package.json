{
  "name": "react-antd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "compile": "better-npm-run compile",
    "compile:release": "better-npm-run compile:release",
    "compile:develop": "better-npm-run compile:develop",
    "compile:master": "better-npm-run compile:master",
    "compile:uat": "better-npm-run compile:uat",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "githook": "cp .hooks/* .git/hooks/"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:master": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:uat": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "uat",
        "DEBUG": "app:*"
      }
    },
    "compile:develop": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "devRemote",
        "DEBUG": "app:*"
      }
    },
    "compile:release": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore build --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:develop": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:master": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:uat": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+http://112.124.41.46/fe-group/industrialpark_web.git"
  },
  "author": "nongjiawu@xinguangnet.com",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.9.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
